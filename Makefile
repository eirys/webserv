debug:	$(eval CXXFLAGS += $(DEBUG_MODE)) all

cookie:
		$(eval CXXFLAGS += $(BONUS_MODE))

bonus:		cookie re


clean:
		@$(RM) $(OBJS_DIR)
		@echo "$(CUT)[deleting objs]"
		@printf "$(UP)"

fclean:		clean
		@$(RM) $(NAME)
		@echo "$(CUT)[deleting $(NAME)]"

re:			fclean all

test: fclean all
	valgrind -s --track-fds=yes --leak-check=full ./$(NAME)

.PHONY:		all clean fclean re

RESET		= \033[0m
RED			= \033[1;31m
GREEN		= \033[1;32m
YELLOW		= \033[1;33m
BLUE		= \033[1;34m
WHITE		= \033[1;37m
ORANGE		= \033[0;38;5;208m
UP			= \033[A
CUT			= \033[K
